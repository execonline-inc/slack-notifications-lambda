{
  "name": "slack-notifications-lambda",
  "version": "1.1.0",
  "description": "Slack notifications lambda from an SNS message using cooperTS",
  "main": "./.build/handler.js",
  "scripts": {
    "build": "parcel build src/handler.ts",
    "check": "tsc --noEmit",
    "slackNotifierTest": "yarn run build && serverless invoke local --function slackNotifierTest",
    "deploy:slackNotifierTest": "sls deploy function -f slackNotifierTest",
    "clean": "rm -rf .serverless",
    "sls": "node --inspect-brk=9229 $(which sls)"
  },
  "keywords": [
    "aws",
    "lambda",
    "slack",
    "cooperTS"
  ],
  "author": "ExecOnline",
  "license": "UNLICENSED",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.3.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/lambda-tester": "^3.5.1",
    "@types/node": "^10.12.18",
    "aws-sdk": "^2.382.0",
    "lambda-tester": "^3.5.0",
    "parcel": "^2.0.0-beta.3.1",
    "prettier": "^1.19.1",
    "serverless": "^2.43.0",
    "serverless-offline": "^7.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@execonline-inc/environment": "^4.0.0",
    "acorn": "^7.1.1",
    "ajaxian": "^3.0.1",
    "ajaxios": "^1.0.4",
    "axios": "^0.21.1",
    "handlebars": "^4.7.3",
    "jsonfile": "^5.0.0",
    "jsonous": "^5.1.0",
    "lodash": "^4.17.15",
    "maybeasy": "^2.6.0",
    "minimist": "^1.2.5",
    "mixin-deep": "^2.0.1",
    "resulty": "^3.0.0",
    "serialize-to-js": "^3.1.1",
    "set-value": "^3.0.1",
    "taskarian": "^3.0.0"
  },
  "resolutions": {
    "minimist": ">=0.2.1"
  },
  "engines": {
    "node": "^14.9.0",
    "npm": "^5.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100
  }
}
